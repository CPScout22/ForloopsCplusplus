#include<iostream>
using namespace std;

int main() {


	for (int i = 70; i < 81; i++)
		cout << i << " ";

	cout << endl; 

	
	for (int j = 100; j <= 300; j += 20)
		cout << j << " ";
	
	cout << endl; 

	
	for (int k = 25; k >= -25; k -= 5)
		cout << k << " ";

	cout << endl;

	
	for (int m = 10000; m > 10; m /= 5)
		cout << m << " ";
}
